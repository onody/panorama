= form_for @trade_hourly_stat do |f|
  - if @trade_hourly_stat.errors.any?
    #error_explanation
      %h2= "#{pluralize(@trade_hourly_stat.errors.count, "error")} prohibited this trade_hourly_stat from being saved:"
      %ul
        - @trade_hourly_stat.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :vendor_id
    = f.number_field :vendor_id
  .field
    = f.label :hour
    = f.datetime_select :hour
  .field
    = f.label :avg_price
    = f.text_field :avg_price
  .actions
    = f.submit 'Save'
